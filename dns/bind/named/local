include "/etc/bind/rndc.key";

zone "burrow.lab.sudonters.com" {
        type master;
        file "/var/lib/bind/burrow.lab.sudonters.com";
        allow-update { key rndc-key; };
};

zone "0.31.10.in-addr.arpa" {
        type master;
        file "/var/lib/bind/0.31.10.rev";
        allow-update { key rndc-key; };
};
